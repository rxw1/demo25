# GraphQL schema
#
# https://gqlgen.com/getting-started/

type Product {
  id: ID!
  name: String!
  price: Int!
}

type Order {
  id: ID!
  productId: ID!
  qty: Int!
  createdAt: String!
}

type Query {
  productById(id: ID!): Product
}

type Mutation {
  createOrder(productId: ID!, qty: Int!): Order
}
