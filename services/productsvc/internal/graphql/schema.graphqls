type Product {
  id: ID!
  price: Int!
  name: String!
}

type Order {
  id: ID!
  qty: Int!
  productId: ID!
  eventId: String!
  createdAt: String!
}

type Query {
  productById(productId: ID!): Product
  products: [Product!]!
  orders: [Order!]!
}

type Mutation {
  createOrder(productId: ID!, qty: Int!): Order
}
 